<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.githrd.figurium.order.dao.CartsMapper">

    <insert id="insertCarts" parameterType="carts">
        insert into carts(id, user_id, product_id, quantity, added_time)
        values(
                null,
                #{ user_id },
                #{ product_id },
                #{ phone },
                CURRENT_TIMESTAMP
                )
    </insert>


</mapper>












